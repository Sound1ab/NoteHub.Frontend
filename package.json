  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "pretest": "graphql-codegen --config codegen.test.yml",
    "test": "craco test --env=jsdom",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "test:e2e": "jest --config=test/jest.config.e2e.ts",
    "test:e2e:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --config=test/jest.config.e2e.ts",
    "codegen:playwright": "playwright codegen http://localhost:8080 --save-storage=auth.json",
    "build": "craco --max_old_space_size=4096 build",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "ts": "tsc --noEmit --pretty --noUnusedParameters --noUnusedLocals",
    "codechecks": "yarn ts && yarn lint && yarn test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      "Chrome >= 60",
      "Safari >= 10.1",
      "iOS >= 10.3",
      "Firefox >= 54",
      "Edge >= 15"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": false,
    "resetModules": false
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:testing-library/recommended",
      "jest-dom",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "testing-library"
    ]
  }
}
